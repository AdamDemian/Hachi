{
  "name": "hachi",
  "version": "1.0.0",
  "engines": {
    "node": "14.17.4",
    "yarn": "1.22.15"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "cd backend && nodemon server.js",
    "build": "cd client && yarn add && yarn build",
    "heroku-postbuild": "yarn install-client && yarn build",
    "install-client": "cd client && npm install",
    "client": "yarn --cwd ./client run start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client --prefix client\"",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client --prefix client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdamDemian/Hachi.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AdamDemian/Hachi/issues"
  },
  "homepage": "https://github.com/AdamDemian/Hachi#readme",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.0.4"
  }
}
